+void writeVFD(char* aChar)
+{
+	char mVFDstring[24];
+	char vfdcmd[25];
+	memset( mVFDstring, 0x20, sizeof( mVFDstring ) );
+	
+	memcpy( mVFDstring, aChar, sizeof(aChar) );
+	char buf[32];
+	unsigned long long tmp[3];
+	unsigned long long tmp2[3];
+	unsigned long long tmp3[3];
+	int j=0, i=0, k=0;
+	int length;
+	int index = 0;
+
+	memset(buf, 0, sizeof( buf ) );
+	memset( tmp, 0, sizeof( tmp ) );
+	memset( tmp2, 0, sizeof( tmp2 ) );		
+	memset( tmp3, 0, sizeof( tmp3 ) );
+
+	vfdcmd[0] = 0x21;
+	
+	memcpy( buf, aChar, 24 );
+	memset( vfdcmd + 1, 0x20, 24 );
+	for(i=0; i < 3; i++)
+	{
+		for(j=0; j < 3; j++)
+		{
+
+			tmp[j] = (((unsigned long long)gVFD_CharTable[buf[i*3+j]][0] << (7*8))
+				| ((unsigned long long)gVFD_CharTable[buf[i*3+j]][1] << (6*8))
+				| ((unsigned long long)gVFD_CharTable[buf[i*3+j]][2] << (5*8))
+				| ((unsigned long long)gVFD_CharTable[buf[i*3+j]][3] << (4*8))
+				| ((unsigned long long)gVFD_CharTable[buf[i*3+j]][4] << (3*8))
+				| ((unsigned long long)gVFD_CharTable[buf[i*3+j]][5] << (2*8))
+				| (0x00 << (1*8))
+				| (0x00));
+
+			tmp2[i] |= (tmp[j] >> j);
+
+		}
+		for(k=0; k < 8; k++)
+		{
+			tmp3[i] |= ((tmp2[i] >> (k*8)) & 0xff) << ((7-k)*8);
+		}
+	aChar = (char*)&tmp3[i];
+	memcpy(vfdcmd + 1 + (i*8), aChar, 8);
+	}
+#if (BOARD_VER == 0x4)	
+#if 0
+	for(i=0; i < 25; i++)
+	{
+		NS16550_putc((NS16550_t)CONFIG_SYS_NS16550_COM2,vfdcmd[i]);
+		//printf("vfdcmd[%d]=0x%x\n",i,vfdcmd[i]);
+	}
+#else
+	Write(vfdcmd, 25);
+#endif
+#endif	
+}
